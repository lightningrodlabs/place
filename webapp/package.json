{
  "name": "place",
  "version": "0.2.0-hc-beta.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "tsc": "rm -rf dist && tsc",
    "build": "rm -rf dist && tsc && rollup -c rollup.config.js && cd dist && cp * ../../electron/web",
    "build:watch": "tsc -w --incremental --preserveWatchOutput",
    "start": "cp index.html dist && cp logo.svg dist && concurrently --names tsc,dev-server \"npm run build:watch\" \"web-dev-server --config ./web-dev-server.config.mjs\""
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@web/dev-server": "^0.1.21",
    "@web/rollup-plugin-html": "^1.11.0",
    "@web/rollup-plugin-import-meta-assets": "^1.0.7",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "exits": "^2.0.1",
    "path-exists-cli": "^2.0.0",
    "rollup": "^2.56.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.2.0",
    "tslib": "^2.4.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "@ddd-qc/cell-proxy": "0.13.1",
    "@ddd-qc/lit-happ": "0.13.1",
    "@holochain/client": "0.12.0",
    "@lit-labs/context": "^0.1.3",
    "@place/elements": "file:../webcomponents",
    "lit": "^2.3.0"
  }
}
